package com.psptech.homeservice.entity;

import jakarta.persistence.*;

@Entity
@Table(name = "employee")
public class Admin {
	@Id
    private int id;

    @Column(name = "name", nullable = false)
    private String name;

    // Constructors
    public Admin() {}

    public Admin(int id, String name) {
    	this.id = id;
        this.name = name;       
    }

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}